{
  "backend": "http://localhost:8080",
  "episodes": 500,
  "learning_rate": 1,
  "discount_factor": 0.9,
  "snake_length": 1,
  "board_width": 30,
  "board_height": 30,
  "q_table_initial_value": 5,
  "initial_epsilon_greedy": 0.9,
  "epsilon_factor_per_episode": 0.99,
  "ate_food_reward": 1,
  "snake_moved_reward": -0.01,
  "game_over_reward": -1,
  "replay_memory_limit": 100,
  "batch_size": 256,
  "gamma": 0.99,
  "gym_environment": "snake-v1",
  "policy": "epsilon_greedy_max_policy",
  "network": "dqn-architecture.json",
  "optimizer": "adam",
  "enable_tensorboard": false,
  "log_directory": "logs"
}
