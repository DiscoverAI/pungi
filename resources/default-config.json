{
  "backend": "http://localhost:8080",
  "episodes": 1000,
  "learning_rate": 1,
  "discount_factor": 0.9,
  "snake_length": 1,
  "board_width": 10,
  "board_height": 10,
  "q_table_initial_value": 5,
  "initial_epsilon_greedy": 0.8,
  "epsilon_factor_per_episode": 0.99,
  "ate_food_reward": 5,
  "snake_moved_reward": -0.1,
  "game_over_reward": -5,
  "replay_memory_limit": 100,
  "batch_size": 32,
  "gamma": 0.99,
  "gym_environment": "snake-v1",
  "policy": "epsilon_greedy_max_policy",
  "network": "dqn-architecture-dense.json",
  "optimizer": "adam",
  "enable_tensorboard": true,
  "log_directory": "logs"
}
